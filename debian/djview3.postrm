#! /bin/sh

case "$1" in
    remove)
	# Alternatives
	if [ -x /usr/sbin/update-alternatives ] ; then
	    /usr/sbin/update-alternatives --quiet \
	     --remove djview /usr/bin/djview3
	fi

	# Menu entries
	if [ -x /usr/bin/update-desktop-database ] ; then
	    /usr/bin/update-desktop-database
	fi
	;;
esac

